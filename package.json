{
  "name": "ai-voice-agent",
  "version": "1.0.0",
  "description": "AI Voice Agent for outbound calling with natural conversations, objection handling, and appointment scheduling",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd src/backend && nodemon server.js",
    "dev:frontend": "cd src/frontend && npm run dev",
    "start": "node src/backend/server.js",
    "test": "jest",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd src/frontend && npm run build",
    "build:backend": "cd src/backend && npm run build"
  },
  "keywords": [
    "ai",
    "voice",
    "agent",
    "outbound",
    "calling",
    "sales",
    "automation"
  ],
  "author": "AI Voice Agent Team",
  "license": "MIT",
  "dependencies": {
    "@twilio/conversations": "^2.0.0",
    "express": "^4.18.2",
    "mongoose": "^7.0.0",
    "redis": "^4.6.0",
    "openai": "^4.0.0",
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "helmet": "^6.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "twilio": "^4.12.0",
    "agenda": "^5.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "jest": "^29.5.0",
    "supertest": "^6.3.3"
  }
}